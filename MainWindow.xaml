<Window x:Class="DemExamFTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemExamFTest"
        mc:Ignorable="d"
        Title="Путешествуй по России" Height="750" Width="1100" MinHeight="750" MinWidth="1100" WindowStartupLocation="CenterScreen" Icon="/Resources/Снимок.PNG">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="138*"/>
            
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background=" #bae3e8 "/>
        <Button x:Name="BtnBck" HorizontalAlignment="Right" Margin="0 0 10 0" Content="Назад" Click="BtnBck_Click" Visibility="Hidden"/>
        <Image Source="/Resources/Снимок.PNG" Height="80" HorizontalAlignment="Left" Margin="10,10,0,10"/>
        <TextBlock Text="Путешествуй по России" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="25" FontFamily="Comic Sans MS"/>
        <Frame x:Name="MainFrame" Grid.Row="1" Grid.RowSpan="2" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered"></Frame>

    </Grid>
</Window>
